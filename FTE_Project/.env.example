# OpenRouter API Configuration
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Model Selection (free models available)
# Options: meta-llama/llama-3.1-8b-instruct:free, google/gemma-2-9b-it:free, mistralai/mistral-7b-instruct:free
OPENROUTER_MODEL=meta-llama/llama-3.1-8b-instruct:free

# Application Settings
APP_NAME=FTE_Agent
LOG_LEVEL=INFO

# Vault Paths
VAULT_INBOX=vault/inbox
VAULT_PROCESSED=vault/processed
VAULT_ACTIONS=vault/actions
VAULT_LOGS=vault/logs
